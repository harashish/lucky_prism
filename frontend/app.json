{
  "expo": {
    "name": "lucky prism",
    "slug": "lucky-prism",
    "scheme": "lucky-prism",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lucky-prism-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E2A42"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/front.png",
        "backgroundColor": "#2E2A42"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.luckyprism"
    },
    "web": {
      "favicon": "./assets/lucky-prism-icon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "2aad687e-ed05-4231-aa4c-de8f57669c60"
      }
    }
  }
}
