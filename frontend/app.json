{
  "expo": {
    "name": "lucky prism",
    "slug": "lucky-prism",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lucky-prism-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E2A42"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/front.png",
        "backgroundColor": "#2E2A42"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/lucky-prism-icon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
